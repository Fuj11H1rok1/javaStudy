//	ここからメソッド
	
//	====職員リスト====
	static void id(int i) {
		String [] name = {"岩鬼","殿馬","里中","山田","微笑","上下","蛸田","高代","香車"};
//		=== int max はnameの要素数 ===
//		int max = name.length -1;
		System.out.println(name[i]);

	}
//	====在庫管理====
	static int zaiko(int total,int out) {
			if(total<10)
				System.out.println("補充してください。");
			return total - out;
		}
	}
	


